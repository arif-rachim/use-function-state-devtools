{
  "name": "Function State Devtools",
  "author" : "Gal2729",
  "version": "1.0",
  "description": "Devtools to monitor Esnaad FunctionState",
  "minimum_chrome_version": "10.0",
  "background": { "scripts": ["background.js"] },
  "manifest_version": 2,
  "icons" : {
    "16" : "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ]
}
